{
  "expo": {
    "name": "ELDI",
    "slug": "ELDI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/eldi-mobile.png",
    "scheme": "eldi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ELDI",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to provide real-time tracking."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/eldi-mobile.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.anonymous.ELDI"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/eldi.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow app to use your location."
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/eldi-mobile.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-asset",
      "expo-maps"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8ddc9063-a9c4-4886-8d91-08fe992a09f8"
      }
    },
    "owner": "david343"
  }
}
